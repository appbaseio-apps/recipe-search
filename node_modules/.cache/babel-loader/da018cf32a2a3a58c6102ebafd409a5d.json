AA,aAAA,EAAA;AACA,MAAA,IAAA,EAAA,QADA;AAEA,MAAA,OAFA,sBAEA;AACA,eAAA;AAAA,iBAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,yBAAA,CAAA;AAAA,SAAA;AACA;AAJA;AA9BA,GAPA;AA4CA,EAAA,IA5CA,kBA4CA;AACA,WAAA;AACA,MAAA,WAAA,EAAA;AADA,KAAA;AAGA,GAhDA;AAiDA,EAAA,OAAA,EAAA;AACA,IAAA,uBADA,mCACA,YADA,EACA;AACA,aAAA,KAAA,CAAA,OAAA,CAAA,YAAA,KAAA,CAAA,YAAA,SAAA,IAAA,YAAA,WAAA,YAAA,YAAA,CAAA,MAAA,IAAA,CAAA,GACA,YAAA,CAAA,GAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,GAAA;AAAA,OAAA,CADA,GAEA,EAFA;AAGA,KALA;AAMA,IAAA,YANA,0BAMA;AACA,UAAA,IAAA,CAAA,KAAA,CAAA,KAAA,OAAA,CAAA,eAAA,GAAA,KAAA,IAAA,KAAA,KAAA,WAAA,EAAA;AACA,aAAA,WAAA,IAAA,CAAA;AACA,aAAA,OAAA,CAAA,KAAA,WAAA,GAAA,KAAA,IAAA;AACA;AACA;AAXA;AAjDA,CAAA","sourcesContent":["<template>\n  <a-spin :spinning=\"loading\" size=\"large\">\n    <!-- empty or no results -->\n    <a-empty v-if=\"results.data.length <= 0\" description=\"\">{{\n      loading ? \"Fetching Data!\" : \"No Results Found!\"\n    }}</a-empty>\n    <!-- renderResults -->\n    <a-col span=\"24\" class=\"result-time-status\">\n      <p>\n        <strong>\n          {{\n            new Intl.NumberFormat(\"en-US\", {\n              maximumSignificantDigits: 3,\n            }).format(results.numberOfResults)\n          }}\n        </strong>\n        results found in\n        <strong>\n          {{\n            typeof results.time == \"object\" ? results.time.took : results.time\n          }}\n        </strong>\n        ms\n      </p></a-col\n    >\n    <a-col\n      id=\"under-observation\"\n      \n    >\n      <!-- rendering the promoted results on top -->\n      <template v-if=\"!!results.promoted\">\n        <template\n          v-for=\"promotedItem in getPromotedResultsArray(results.promotedData)\"\n        >\n          <promoted-card-item\n            :key=\"promotedItem.id\"\n            :promoted-item=\"promotedItem\"\n          />\n        </template>\n      </template>\n\n      <template v-for=\"(recipeItem, index) in results.data\">\n        <!-- rendering all the result item except the promoted ones -->\n        <card-item\n          v-if=\"index !== 0\"\n          :key=\"index\"\n          :recipe-item=\"recipeItem\"\n          :set-full-recipe=\"setFullRecipe\"\n        />\n      </template>\n\n      <pagination-trigger\n        :isSpinning=\"loading && results.data.length > 0\"\n        :scrollToTop=\"!from\"\n        :callNextPage=\"callNextPage\"\n      />\n    </a-col>\n  </a-spin>\n</template>\n\n<script>\nimport PaginationTrigger from './PaginationTrigger.vue';\nimport CardItem from './CardItem.vue';\nimport PromotedCardItem from './PromotedCardItem.vue';\n\nexport default {\n\tname: 'ResultsRenderer',\n\tcomponents: {\n\t\tCardItem,\n\t\tPromotedCardItem,\n\t\tPaginationTrigger,\n\t},\n\tprops: {\n\t\tloading: Boolean,\n\t\tsize:{\n\t\t\ttype: Number,\n\t\t\tdefault() {\n\t\t\t\treturn 10;\n\t\t\t},\n\t\t},\n\t\tresults: {\n\t\t\ttype: Object,\n\t\t\tdefault() {\n\t\t\t\treturn [];\n\t\t\t},\n\t\t},\n\t\tfrom: {\n\t\t\ttype: Number,\n\t\t\tdefault() {\n\t\t\t\treturn 0;\n\t\t\t},\n\t\t},\n\t\t/* eslint-disable no-console */\n\t\tsetFrom: {\n\t\t\ttype: Function,\n\t\t\tdefault() {\n\t\t\t\treturn () => {\n\t\t\t\t\tconsole.log('Please provide a function');\n\t\t\t\t};\n\t\t\t},\n\t\t},\n\t\t/* eslint-enable no-console */\n\t\tsetFullRecipe: {\n\t\t\ttype: Function,\n\t\t\tdefault() {\n\t\t\t\treturn ()=>window.console.log('Please provide Handler ');\n\t\t\t},\n\t\t},\n\t},\n\tdata() {\n\t\treturn {\n\t\t\tcurrentPage:0\n\t\t};\n\t},\n\tmethods: {\n\t\tgetPromotedResultsArray(promotedData) {\n\t\t\treturn Array.isArray(promotedData) && promotedData?.length > 0\n\t\t\t\t? promotedData.map((item) => item.doc)\n\t\t\t\t: [];\n\t\t},\n\t\tcallNextPage(){\n\t\t\tif (Math.floor(this.results.numberOfResults / this.size) >= this.currentPage) {\n\t\t\t\tthis.currentPage += 1;\n\t\t\t\tthis.setFrom(this.currentPage * this.size);\n\t\t\t}\n\t\t}\n\t},\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n.result-time-status p strong{\n    color: red;\n}\n\n#under-observation {\n  display: flex;\n  flex-wrap: wrap;\n  grid-gap: 24px;\n  max-height: 60vh;\n  overflow: hidden scroll;\n\n\n\n}\n\n@media screen and (max-width: 600px) {\n   #under-observation{  max-height: 50vh;}\n}\n</style>\n"],"sourceRoot":"src/components"}]}